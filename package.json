{
  "name": "bpanel",
  "version": "0.1.0-beta",
  "description": "GUI application to interact with a Bcoin Bitcoin node",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.config.js --watch",
    "start": "nodemon ./server/server.js",
    "start:dev": "NODE_ENV=development nodemon ./server/server.js",
    "start:docker": "node ./scripts/docker-config-init.js && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,json,css,jsx,scss}": ["prettier --write", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "git@git.purse.io:bcoin/bpanel.git"
  },
  "keywords": ["bcoin", "bitcoin", "GUI"],
  "author": "bcoin",
  "license": "MIT",
  "dependencies": {
    "bcoin": "1.0.0-beta.15",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "bpanel-ux": "git+https://github.com/Bucko13/bpanel-ux.git"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "prettier": "1.7.4",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "webpack": "^3.8.1"
  }
}
